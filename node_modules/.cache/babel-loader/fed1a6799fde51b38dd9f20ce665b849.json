A,CAAA,UAAA,UAAA,GAAA,UAAA,GAAA,IAAA,CAAA;AACA,OAXA;AAYA,KAtBA;AAwBA,IAAA,QAxBA,sBAwBA;AACA;AACA,UAAA,QAAA,GAAA,IAAA,IAAA,GAAA,kBAAA,EAAA;AACA,UAAA,QAAA,GAAA,IAAA,IAAA,GAAA,kBAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAAA,QAAA,EAJA,CAKA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,mDAAA,KAAA,QAAA,GAAA,OAAA,EAAA;AACA,QAAA,IAAA,EAAA;AACA,UAAA,IAAA,EAAA,QADA;AAEA,UAAA,IAAA,EAAA,QAFA;AAGA,UAAA,IAAA,EAAA;AAHA;AADA,OAAA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,QAAA,KAAA,CAAA,UAAA,QAAA,GAAA,QAAA,GAAA,IAAA,CAAA;AACA,OAVA;AAYA,KA1CA;AA2CA,IAAA,QA3CA,sBA2CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,mDAAA,KAAA,CAAA,QAAA,GAAA,OAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;AAGA,gBAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA;AAhDA;AARA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"start_work\">出勤</button>\r\n    <button @click=\"end_work\">退勤</button>\r\n    <button @click=\"datalist\">履歴</button>\r\n    <div>\r\n      <table>\r\n        <tr>\r\n          <th>出勤</th>\r\n          <th>退勤</th>\r\n        </tr>\r\n        <tr v-for=\"(item,index) in worklist\" :key=\"index\">\r\n          <td v-if=\"item.data.number == 0\">{{item.data.date}}  {{item.data.time}}</td>\r\n          <td>{{item.data.date}}  {{item.data.time}}</td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  data(){\r\n    return{\r\n      worklist:[],\r\n      username:\"\",\r\n    }\r\n\r\n  },\r\n  methods:{\r\n    start_work(){\r\n      //一時的に保存したデータを取得\r\n      this.username = sessionStorage.getItem('name')\r\n      console.log(this.username)\r\n      //現在の年月日時を取得\r\n      var start_date = new Date().toLocaleDateString()\r\n      var start_time = new Date().toLocaleTimeString()\r\n      console.log(start_date,start_time)\r\n      //データベースにデータを送る\r\n      axios.post(\"https://fir-795b6-default-rtdb.firebaseio.com/\" + this.username +\".json\",{\r\n        data:{\r\n          date:start_date,\r\n          time:start_time,\r\n          type:'出勤',\r\n          number:0\r\n        },\r\n      })\r\n      .then(response =>{\r\n        console.log(response)\r\n        alert(\"出勤時間は\" + start_date+start_time + \"です\")\r\n      })\r\n    },\r\n\r\n    end_work(){\r\n      //現在の年月日時を取得\r\n      var end_date = new Date().toLocaleDateString()\r\n      var end_time = new Date().toLocaleTimeString()\r\n      console.log(end_date,end_time)\r\n      //データベースにデータを送る(動的にURLを変更)\r\n      axios.post(\"https://fir-795b6-default-rtdb.firebaseio.com/\"+ this.username +\".json\",{\r\n        data:{\r\n          date:end_date,\r\n          time:end_time,\r\n          type:'退勤',\r\n        },\r\n      })\r\n      .then(response =>{\r\n        console.log(response)\r\n        alert(\"出勤時間は\" + end_date + end_time + \"です\")\r\n      })\r\n\r\n    },\r\n    async datalist(){\r\n      //データベースからデータを取得(動的にURLを変更)\r\n     var box = await axios.get(\"https://fir-795b6-default-rtdb.firebaseio.com/\"+ this.username +\".json\")\r\n     this.worklist = box.data\r\n     console.log(this.worklist)\r\n    }\r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}