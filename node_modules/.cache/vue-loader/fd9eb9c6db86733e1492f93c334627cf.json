r\n      .then(res=>{\r\n        console.log(res.data)\r\n      })\r\n    },\r\n\r\n    //通信ライブラリの受信メソッド\r\n    datalist(){\r\n      axios.get(\"https://fir-795b6-default-rtdb.firebaseio.com/fir-795b6-default-rtdb.json\")\r\n      .then(response => {\r\n        console.log('body:', response);\r\n        this.work_list = response.data \r\n      }).catch(error=>{\r\n        console.log(error)\r\n      })\r\n    },\r\n\r\n    //非同期処理 createlistメソッド\r\n    async createlist(){\r\n      try{\r\n        var data1 = await axios.get(\"https://fir-795b6-default-rtdb.firebaseio.com/fir-795b6-default-rtdb/-Mo8X1oVl5vtpvKv1qbz.json\")\r\n        console.log(data1)\r\n        this.work_list = null\r\n        this.work_list = data1.data\r\n        console.log(this.work_list)\r\n      }catch(error){\r\n        console.log(error)\r\n      }\r\n\r\n      \r\n    }\r\n  },\r\n\r\n}\r\n</script>"]}]}